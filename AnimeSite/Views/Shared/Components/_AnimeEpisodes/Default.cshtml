@using EntityLayer.DTO
@model List<AnimesDetailEpisode>


<div class="row">
    <div class="col-lg-12">
        <div class="anime__details__episodes">
            <div class="section-title">
                <h5>Bölümler</h5>
            </div>
            @foreach (var item in Model)
            {
                <a href="/Anime/AnimeWatching/@item.Id">@item.AnimeName @item.EpisodeName</a>
            }
        </div>
    </div>
</div>
